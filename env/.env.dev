# 开发环境配置
NODE_ENV=development
VITE_APP_ENV=dev
VITE_APP_TITLE=Vue3项目 - 开发环境

# API配置
VITE_API_BASE_URL=https://dev-api.example.com
VITE_API_TIMEOUT=15000
VITE_API_PREFIX=/api/v1

# 功能开关
VITE_ENABLE_MOCK=true
VITE_ENABLE_DEBUG=true
VITE_ENABLE_CONSOLE=true
VITE_ENABLE_VCONSOLE=false

# 第三方服务
VITE_OSS_BUCKET=dev-bucket
VITE_CDN_URL=https://dev-cdn.example.com
VITE_UPLOAD_URL=https://dev-upload.example.com

# 监控配置
VITE_SENTRY_DSN=https://dev-sentry-dsn@sentry.io/project
VITE_ANALYTICS_ID=GA-DEV-XXXXXXX

# 其他配置
VITE_APP_VERSION=1.0.0-dev
VITE_BUILD_TIME=
VITE_GIT_HASH=