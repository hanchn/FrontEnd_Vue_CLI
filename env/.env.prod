# 生产环境配置
NODE_ENV=production
VITE_APP_ENV=prod
VITE_APP_TITLE=Vue3项目

# API配置
VITE_API_BASE_URL=https://api.example.com
VITE_API_TIMEOUT=5000
VITE_API_PREFIX=/api/v1

# 功能开关
VITE_ENABLE_MOCK=false
VITE_ENABLE_DEBUG=false
VITE_ENABLE_CONSOLE=false
VITE_ENABLE_VCONSOLE=false

# 第三方服务
VITE_OSS_BUCKET=prod-bucket
VITE_CDN_URL=https://cdn.example.com
VITE_UPLOAD_URL=https://upload.example.com

# 监控配置
VITE_SENTRY_DSN=https://prod-sentry-dsn@sentry.io/project
VITE_ANALYTICS_ID=GA-PROD-XXXXXXX

# 其他配置
VITE_APP_VERSION=1.0.0
VITE_BUILD_TIME=
VITE_GIT_HASH=