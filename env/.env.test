# 测试环境配置
NODE_ENV=test
VITE_APP_ENV=test
VITE_APP_TITLE=Vue3项目 - 测试环境

# API配置
VITE_API_BASE_URL=https://test-api.example.com
VITE_API_TIMEOUT=10000
VITE_API_PREFIX=/api/v1

# 功能开关
VITE_ENABLE_MOCK=true
VITE_ENABLE_DEBUG=true
VITE_ENABLE_CONSOLE=true
VITE_ENABLE_VCONSOLE=true
VITE_ENABLE_ROUTE_NAV=true

# 第三方服务
VITE_OSS_BUCKET=test-bucket
VITE_CDN_URL=https://test-cdn.example.com
VITE_UPLOAD_URL=https://test-upload.example.com

# 监控配置
VITE_SENTRY_DSN=
VITE_ANALYTICS_ID=

# 其他配置
VITE_APP_VERSION=1.0.0-test
VITE_BUILD_TIME=
VITE_GIT_HASH=